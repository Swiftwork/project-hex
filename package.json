{
  "name": "project-hex",
  "version": "1.0.0",
  "description": "Turnbased strategy game in typescript",
  "main": "dist/index.js",
  "scripts": {
    "start": "rimraf debug/* && webpack-dev-server --config .config/debug.config.js",
    "build:dist": "rimraf dist/* && webpack --config .config/dist.config.js",
    "build:debug": "rimraf debug/* && webpack --config .config/debug.config.js",
    "deploy": "npm run build:dist && ./deploy.sh"
  },
  "keywords": [
    "isomorphic",
    "game",
    "typescript"
  ],
  "author": "Erik Hughes",
  "license": "CC-BY-NC-ND-4.0",
  "dependencies": {
    "babylonjs": "git+https://github.com/BabylonJS/Babylon.js.git",
    "babylonjs-gui": "^3.1.0-alpha3.4",
    "core-js": "2.5.1",
    "fontfaceobserver": "2.0.13",
    "handjs": "1.3.11",
    "socket.io": "2.0.3",
    "socket.io-client": "2.0.3",
    "socket.io-p2p": "2.2.0",
    "socket.io-p2p-server": "1.2.0"
  },
  "devDependencies": {
    "@types/core-js": "0.9.43",
    "@types/node": "8.0.30",
    "@types/socket.io": "1.4.30",
    "@types/socket.io-client": "1.4.30",
    "awesome-typescript-loader": "3.2.3",
    "babylon-file-loader": "0.1.0",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "path": "0.12.7",
    "pug": "2.0.0-rc.4",
    "pug-loader": "2.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "typescript": "2.5.2",
    "webpack": "3.6.0",
    "webpack-dashboard": "1.0.0-5",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0"
  }
}
