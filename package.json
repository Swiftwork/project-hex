{
  "name": "project-hex",
  "version": "1.0.0",
  "description": "Turnbased strategy game in typescript",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base build --inline --progress --host 0.0.0.0 --port 8080",
    "build:dev": "rimraf build && webpack --progress --profile --env.development",
    "build:prod": "rimraf build && webpack --progress --profile --bail --env.production",
    "deploy": "npm run build:prod && ./deploy.sh"
  },
  "keywords": [
    "isomorphic",
    "game",
    "typescript"
  ],
  "author": "Erik Hughes",
  "license": "CC-BY-NC-ND-4.0",
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/node": "7.0.0",
    "@types/socket.io": "1.4.27",
    "@types/socket.io-client": "1.4.28",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "babylon-file-loader": "0.1.0",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "path": "0.12.7",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "typescript": "2.0.6",
    "typings": "2.1.0",
    "webpack": "2.1.0-beta.24",
    "webpack-dev-server": "2.1.0-beta.10",
    "webpack-merge": "2.4.0"
  },
  "dependencies": {
    "babylonjs": "git+https://github.com/Swiftwork/Babylon.js-nightly.git",
    "core-js": "2.4.1",
    "fontfaceobserver": "2.0.5",
    "handjs": "1.3.11",
    "socket.io": "1.5.1",
    "socket.io-client": "1.5.1",
    "socket.io-p2p": "2.2.0",
    "socket.io-p2p-server": "1.2.0"
  }
}